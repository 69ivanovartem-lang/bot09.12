import sqlite3

def create_minimal_data():
    """–°–æ–∑–¥–∞–µ—Ç –º–∏–Ω–∏–º–∞–ª—å–Ω—ã–π –Ω–∞–±–æ—Ä —Ç–µ—Å—Ç–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö"""
    
    conn = sqlite3.connect('zettelkasten.db')
    cursor = conn.cursor()
    
    # –û—á–∏—â–∞–µ–º –±–∞–∑—É
    cursor.execute('DELETE FROM note_links')
    cursor.execute('DELETE FROM notes')
    
    # –¢–µ—Å—Ç–æ–≤—ã–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å
    user_id = 123456789
    
    # –°–æ–∑–¥–∞–µ–º –Ω–µ—Å–∫–æ–ª—å–∫–æ —Å–≤—è–∑–∞–Ω–Ω—ã—Ö –∑–∞–º–µ—Ç–æ–∫
    notes_data = [
        {
            'title': '–ò–¥–µ—è –¥–ª—è –ø—Ä–æ–µ–∫—Ç–∞',
            'content': '–†–∞–∑—Ä–∞–±–æ—Ç–∞—Ç—å —Å–∏—Å—Ç–µ–º—É —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –∑–∞–º–µ—Ç–∫–∞–º–∏ —Å –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å—é —Å–æ–∑–¥–∞–Ω–∏—è —Å–≤—è–∑–µ–π –º–µ–∂–¥—É –Ω–∏–º–∏.',
            'tags': '–ø—Ä–æ–µ–∫—Ç, —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∞, –∏–¥–µ—è'
        },
        {
            'title': '–¢–µ—Ö–Ω–∏—á–µ—Å–∫–∏–µ —Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è',
            'content': '–°–∏—Å—Ç–µ–º–∞ –¥–æ–ª–∂–Ω–∞ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞—Ç—å:\n‚Ä¢ –°–æ–∑–¥–∞–Ω–∏–µ –∑–∞–º–µ—Ç–æ–∫\n‚Ä¢ –°–≤—è–∑—ã–≤–∞–Ω–∏–µ –∑–∞–º–µ—Ç–æ–∫\n‚Ä¢ –ü–æ–∏—Å–∫ –ø–æ —Å–æ–¥–µ—Ä–∂–∏–º–æ–º—É',
            'tags': '—Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è, —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∞'
        },
        {
            'title': '–ê—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞ —Å–∏—Å—Ç–µ–º—ã',
            'content': '–ü—Ä–µ–¥–ª–∞–≥–∞–µ–º–∞—è –∞—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞:\n‚Ä¢ Backend: Python + SQLite\n‚Ä¢ Frontend: Telegram Bot\n‚Ä¢ –ë–∞–∑–∞ –¥–∞–Ω–Ω—ã—Ö: SQLite —Å —Ç–∞–±–ª–∏—Ü–∞–º–∏ notes –∏ note_links',
            'tags': '–∞—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞, —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∞'
        },
        {
            'title': 'Zettelkasten –º–µ—Ç–æ–¥',
            'content': 'Zettelkasten - —Å–∏—Å—Ç–µ–º–∞ –≤–µ–¥–µ–Ω–∏—è –∑–∞–º–µ—Ç–æ–∫, —Ä–∞–∑—Ä–∞–±–æ—Ç–∞–Ω–Ω–∞—è —Å–æ—Ü–∏–æ–ª–æ–≥–æ–º –ù–∏–∫–ª–∞—Å–æ–º –õ—É–º–∞–Ω–æ–º.\n\n–û—Å–Ω–æ–≤–Ω—ã–µ –ø—Ä–∏–Ω—Ü–∏–ø—ã:\n‚Ä¢ –ê—Ç–æ–º–∞—Ä–Ω–æ—Å—Ç—å –∏–¥–µ–π\n‚Ä¢ –°–≤—è–∑—ã–≤–∞–Ω–∏–µ –∑–∞–º–µ—Ç–æ–∫\n‚Ä¢ –ù–µ–ª–∏–Ω–µ–π–Ω–æ–µ –º—ã—à–ª–µ–Ω–∏–µ',
            'tags': 'zettelkasten, –º–µ—Ç–æ–¥–æ–ª–æ–≥–∏—è, –æ—Ä–≥–∞–Ω–∏–∑–∞—Ü–∏—è'
        }
    ]
    
    note_ids = []
    
    # –í—Å—Ç–∞–≤–ª—è–µ–º –∑–∞–º–µ—Ç–∫–∏
    for note in notes_data:
        cursor.execute('''
            INSERT INTO notes (user_id, title, content, tags)
            VALUES (?, ?, ?, ?)
        ''', (user_id, note['title'], note['content'], note['tags']))
        note_ids.append(cursor.lastrowid)
    
    # –°–æ–∑–¥–∞–µ–º —Å–≤—è–∑–∏ –º–µ–∂–¥—É –∑–∞–º–µ—Ç–∫–∞–º–∏
    links = [
        (note_ids[0], note_ids[1]),  # –ò–¥–µ—è -> –¢—Ä–µ–±–æ–≤–∞–Ω–∏—è
        (note_ids[0], note_ids[2]),  # –ò–¥–µ—è -> –ê—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞
        (note_ids[1], note_ids[2]),  # –¢—Ä–µ–±–æ–≤–∞–Ω–∏—è -> –ê—Ä—Ö–∏—Ç–µ–∫—Ç—É—Ä–∞
        (note_ids[0], note_ids[3]),  # –ò–¥–µ—è -> Zettelkasten –º–µ—Ç–æ–¥
        (note_ids[3], note_ids[1]),  # Zettelkasten –º–µ—Ç–æ–¥ -> –¢—Ä–µ–±–æ–≤–∞–Ω–∏—è
    ]
    
    for from_id, to_id in links:
        cursor.execute('''
            INSERT INTO note_links (from_note_id, to_note_id)
            VALUES (?, ?)
        ''', (from_id, to_id))
    
    conn.commit()
    conn.close()
    
    print("‚úÖ –ú–∏–Ω–∏–º–∞–ª—å–Ω—ã–µ —Ç–µ—Å—Ç–æ–≤—ã–µ –¥–∞–Ω–Ω—ã–µ —Å–æ–∑–¥–∞–Ω—ã!")
    print(f"üìù –°–æ–∑–¥–∞–Ω–æ –∑–∞–º–µ—Ç–æ–∫: {len(note_ids)}")
    print(f"üîó –°–æ–∑–¥–∞–Ω–æ —Å–≤—è–∑–µ–π: {len(links)}")
    print("\nüéØ –¢–µ—Å—Ç–∏—Ä—É–π—Ç–µ –∫–æ–º–∞–Ω–¥—ã:")
    print("   /notes - —Å–ø–∏—Å–æ–∫ –∑–∞–º–µ—Ç–æ–∫")
    print("   /tree - –¥–µ—Ä–µ–≤–æ —Å–≤—è–∑–µ–π")
    print("   /graph - –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è –≥—Ä–∞—Ñ–∞")

if __name__ == "__main__":
    create_minimal_data()